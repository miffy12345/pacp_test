	<!-- 加载系统代码相关的SQL -->
	<entry key="platform.code_load_sql">
		SELECT DMJBH AS SUBJECT,
		       DM    AS CODEID,
		       DMMC  AS VALUE,
		       ORGID,
		       PXH,
		       SFYX  AS VALID,
		       FDM   AS PARENTCODEID,
		       BZ
		  FROM T_PLATFORM_GGDMZ
		 ORDER BY SUBJECT, PXH, CODEID;
		SELECT FDM AS SUBJECT,
		       XZQHDM AS CODEID,
		       XZQH AS VALUE,
		       ORGID,
		       FDM AS PARENTCODEID,
		       PXM AS PXH,
		       '1' AS VALID
		  FROM T_PLATFORM_XZQHDM
		 ORDER BY PXM
	</entry>
